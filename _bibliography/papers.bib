	date-modified = {2022-05-14 21:00:35 -0400},
	file = {:Users/mcrowley/Dropbox/MendeleyDesktop/Hall et al. - 2012 - Managing Invasive Species in a River Network.pdf:pdf},
	in-cv = {1},
	keywords = {computational-sustainability,ecology,invasive-species-management,Reinforcement-Learning,value iteration},
